<UserControl x:Class="Wpf.App.Controls.Erfassung.ErfassungControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Wpf.App.Controls.Erfassung"
			 xmlns:ViewModel="clr-namespace:Datenerfassung.ViewModel;assembly=Datenerfassung.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
	<StackPanel Orientation="Vertical">
		<Label>Vorname:</Label>
		<TextBox Text="{Binding SelectedFreund.FirstName}"></TextBox>
	</StackPanel>
</UserControl>
